    "@typescript-eslint/parser": "^4.27.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "standard": "^16.0.3"
  },
  "resolutions": {
    "rdf-loaders-registry": "0.3.0"
